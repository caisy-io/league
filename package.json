{
  "name": "@caisy/league",
  "version": "0.1.16",
  "access": "public",
  
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "prepare": "install-peers",
    "build:tokens": "node scripts/sd.build.js",
    "prepublishOnly": "yarn build",
    "postbuild": "dts-bundle-generator --no-check --external-types=react --external-imports=moment -o dist/types.d.ts src/index.ts",
    "build:cp": "rimraf dist && yarn build && rimraf ../caisy-next/node_modules/@caisy/league/dist && ncp ./dist ../caisy-next/node_modules/@caisy/league/dist",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "lint:types": "tsc --project tsconfig.json --noEmit",
    "lint:fix": "eslint ./src/**/*.ts ./src/**/*.tsx --fix",
    "dev": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "build": "rollup -c",
    "build:storybook": "yarn build-storybook --loglevel warn 2> ./sb-out.txt && sh ./run-sb-exit.sh"
  },
  "sideEffects": false,
  "devDependencies": {
    "@caisy/eslint-config-caisy": "^1.3.0",
    "@caisy/prettier": "^1.14.1",
    "@caisy/tsconfig": "^1.6.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@storybook/addon-actions": "^6.4.0-beta.7",
    "@storybook/addon-essentials": "^6.4.0-beta.7",
    "@storybook/addon-links": "^6.4.0-beta.7",
    "@storybook/react": "^6.4.0-beta.7",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "dts-bundle-generator": "^5.9.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.26.1",
    "install-peers-cli": "^2.2.0",
    "plop": "^2.7.6",
    "rollup": "^2.33.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-ts-paths-resolve": "^1.7.1",
    "storybook-addon-designs": "^6.2.0",
    "style-dictionary": "^3.0.3",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^17",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.2",
    "react-dom": "^17"
  },
  "files": [
    "dist",
    "src"
  ],
  "resolutions": {
    "dayjs": "1.10.5",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.2"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "dayjs": "1.10.5",
    "fuzzysearch": "^1.0.3",
    "react-flatpickr": "^3.10.7",
    "react-lazyload": "^3.2.0",
    "react-table": "^7.7.0",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "styled-components": "5.3.0"
  }
}
